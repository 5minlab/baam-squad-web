{
  "name": "baam-squad-web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.77.6"
  },
  "scripts": {
    "serve-http": "http-server docs -o",
    "serve-https": "http-server docs -S -C key/cert.pem -K key/key.pem -o",
    "watch-css": "sass --watch docs/sass/style.scss:docs/style.css --embed-source-map --embed-sources",
    "build-css": "sass --no-source-map docs/sass/style.scss docs/style.css",
    "build": "npm run build-css",
    "start-http": "npm-run-all -p watch-css serve-http",
    "start-https": "npm-run-all -p watch-css serve-https"
  },
